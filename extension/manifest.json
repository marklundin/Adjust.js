{

    "name": "Adjust",
    "description": "A extensible dev tool for interfacing and adjusting javascript objects.",
    "manifest_version": 2,
    "version": "0.0.1",
    "icons": {
        "16": "assets/icon_32.png",
        "48": "assets/icon_32.png",
        "128": "assets/icon_32.png"
    },
    "devtools_page": "devtools/devtools.html",
    "minimum_chrome_version": "26.0",

    "background": {
        "page": "background/background_page.html",
        "persistent": true
    },

    "permissions": [
        "tabs",
        "experimental",
        "browsingData",
        "http://*/*",
        "https://*/*",
        "file://*"
    ]

}